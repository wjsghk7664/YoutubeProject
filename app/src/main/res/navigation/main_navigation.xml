<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/main_nav_fragment_home">

    <activity
        android:id="@+id/main_activity"
        android:name="com.example.youtubeproject.presentation.ui.MainActivity"
        android:label="activity_sign_in"
        tools:layout="@layout/activity_main" />

    <fragment
        android:id="@+id/main_nav_fragment_home"
        android:name="com.example.youtubeproject.presentation.ui.fragment.HomeFragment"
        android:label="fragment_my_page"
        tools:layout="@layout/fragment_home"
        />

    <fragment
        android:id="@+id/main_nav_fragment_search"
        android:name="com.example.youtubeproject.presentation.ui.fragment.SearchFragment"
        android:label="fragment_my_page"
        tools:layout="@layout/fragment_home"
        />

    <fragment
        android:id="@+id/main_nav_fragment_playlist"
        android:name="com.example.youtubeproject.presentation.ui.fragment.PlaylistFragment"
        android:label="fragment_my_page"
        tools:layout="@layout/fragment_home"
        />

    <fragment
        android:id="@+id/main_nav_fragment_my_page"
        android:name="com.example.youtubeproject.presentation.ui.fragment.MyPageFragment"
        android:label="fragment_my_page"
        tools:layout="@layout/fragment_home"
        />



</navigation>